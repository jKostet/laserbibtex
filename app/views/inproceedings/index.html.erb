<h1>Inproceedings</h1>

<table class="table">
  <thead>
    <tr>
      <th>Reference</th>
      <th>Author</th>
      <th>Title</th>
      <th>Booktitle</th>
      <th>Editor</th>
      <th>Volume</th>
      <th>Number</th>
      <th>Series</th>
      <th>Pages</th>
      <th>Address</th>
      <th>Organization</th>
      <th>Publisher</th>
      <th>Year</th>
      <th>Month</th>
      <th>Note</th>
      <th>Tags</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inproceedings.each do |inproceeding| %>
      <tr>
        <td><%= inproceeding.reference %></td>
        <td><%= inproceeding.author %></td>
        <td><%= inproceeding.title %></td>
        <td><%= inproceeding.booktitle %></td>
        <td><%= inproceeding.editor %></td>
        <td><%= inproceeding.volume %></td>
        <td><%= inproceeding.number %></td>
        <td><%= inproceeding.series %></td>
        <td><%= inproceeding.pages %></td>
        <td><%= inproceeding.address %></td>
        <td><%= inproceeding.organization %></td>
        <td><%= inproceeding.publisher %></td>
        <td><%= inproceeding.year %></td>
        <td><%= inproceeding.month %></td>
        <td><%= inproceeding.note %></td>
        <td>
          <% if inproceeding.tags %>
              <%  inproceeding.tags.split(", ").each do |tag| %>
                  <span class="label label-primary"> <%= tag %> </span>
                  &nbsp;
              <% end %>
          <% end %>
        </td>
        <td><%= link_to 'Show', inproceeding %></td>
        <td><%= link_to 'Edit', edit_inproceeding_path(inproceeding) %></td>
        <td><%= link_to 'Destroy', inproceeding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Inproceeding', new_inproceeding_path %>
