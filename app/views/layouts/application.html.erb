<!DOCTYPE html>
<html>
<head>
  <title>Laserbibtex</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="navibar">
    <%= link_to 'articles', articles_path %>
    <%= link_to 'books', books_path %>
    <%= link_to 'inproceedings', inproceedings_path %>
    |
    <%= link_to 'all references', references_path %>
    <%= link_to '.bibtex', download_path %>
  </div>

<%= yield %>

</body>
</html>
